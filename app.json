{
  "expo": {
    "name": "UTTT",
    "slug": "UTTT",
    "scheme": "Archimedes4.UTTT",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5E17EB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "@bacons/apple-targets",
        {
          "appleTeamId": "SYV2CK2N9N",
          "root": "./targets"
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication",
      [
        "expo-router",
        {
          "headOrigin": "https://archimedes4-games.web.app/"
        }
      ],
      "expo-notifications",
      "expo-secure-store",
      "expo-dev-client",
      "expo-font",
      [
        "expo-asset",
        {
          "assets": [
            "assets/UTTT-Demo-Start.gif",
            "assets/UTTT-Demo-End.gif",
            "assets/UTTT-Demo-Ai.gif",
            "assets/UTTT-Demo-Start.gif",
            "assets/UTTT-Demo-End.gif",
            "assets/UTTT-Demo-Ai.gif"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.0",
            "useFrameworks":"static"
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Archimedes4.UTTT",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSAllowsArbitraryLoads": true,
        "CFBundleAllowMixedLocalizations": true,
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "exp.direct": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            },
            "128.189.128.248": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        },
        "LSMinimumSystemVersion": "17"
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "13",
      "appleTeamId": "SYV2CK2N9N",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.Archimedes4.UTTT"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "Archimedes4.UTTT"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "static",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "50420409-1879-402c-bd37-d2db6871cea8"
      }
    }
  }
}
